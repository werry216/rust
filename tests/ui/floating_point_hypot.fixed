// run-rustfix
#![warn(clippy::suboptimal_flops, clippy::imprecise_flops)]

fn main() {
    let x = 3f32;
    let y = 4f32;
    let _ = x.hypot(y);
    let _ = (x + 1f32).hypot(y);
    let _ = x.hypot(y);
    // Cases where the lint shouldn't be applied
    let _ = x.mul_add(x, y * y).sqrt();
    let _ = x.mul_add(4f32, y * y).sqrt();
}
