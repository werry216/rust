// run-rustfix
#![warn(clippy::imprecise_flops)]

fn main() {
    let one = 1;
    let x = 3f32;
    let _ = x * x;
    let _ = x * x;

    let y = 4f32;
    let _ = (x * x + y).sqrt();
    let _ = (x + y * y).sqrt();
    // Cases where the lint shouldn't be applied
    let _ = x.powi(3);
    let _ = x.powi(one + 1);
    let _ = x.hypot(y);
}
